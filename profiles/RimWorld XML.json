{
  "name": "RimWorld XML",
  "description": "Perfil espec√≠fico para arquivos XML do RimWorld",
  "capture_patterns": [
    "<label>([^<]+)</label>",
    "<description>([^<]+)</description>",
    "<[a-zA-Z_]+>([^<>]+)</[a-zA-Z_]+>"
  ],
  "exclude_patterns": [
    "<defName>.*?</defName>",
    "<!--.*?-->"
  ],
  "file_type": "xml"
}